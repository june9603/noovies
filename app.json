    "android": {
      "package": "com.kkkk___tj.noovies",
      "versionCode": 1
    },
    "androidStatusBar": {
      "barStyle": "dark-content",
      "backgroundColor": "#000000"
    },
    "androidNavigationBar": {
      "barStyle": "dark-content",
      "backgroundColor": "#000000"
    }
  }
